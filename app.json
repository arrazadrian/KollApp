{
  "expo": {
    "name": "KollApp",
    "slug": "KollApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/Images/logoKollApp.png",
    "splash": {
      "image": "./src/assets/Images/splashKollApp.png",
      "resizeMode": "cover",
      "backgroundColor": "#FFF3C8"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/Images/logoKollApp.png",
        "backgroundColor": "#FFF3C8"
      },
      "package": "com.ipb.kollapp",
      "softwareKeyboardLayoutMode": "pan"
    },
    "web": {
      "favicon": "./src/assets/Images/favicon.png"
    }
  }
}
